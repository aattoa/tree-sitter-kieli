{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "kieli",
      "camelcase": "Kieli",
      "title": "Kieli",
      "scope": "source.ki",
      "file-types": [
        "ki"
      ],
      "injection-regex": "^kieli$",
      "class-name": "TreeSitterKieli"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Kieli grammar for tree-sitter",
    "authors": [
      {
        "name": "aattoa",
        "email": "aattoa@proton.me"
      }
    ],
    "links": {
      "repository": "https://github.com/aattoa/tree-sitter-kieli"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}